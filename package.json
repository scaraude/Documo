{
  "name": "documo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && prisma migrate dev && next dev --turbopack | npx pino-pretty --ignore pid,hostname",
    "dev:clean": "rm -rf .next && yarn dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:integration": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test vitest --config=vitest.integration.config.ts",
    "test:integration:watch": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test vitest --config=vitest.integration.config.ts --watch",
    "test:seed": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test npx tsx prisma/seed-test.ts",
    "test:db:up": "docker compose -f docker-compose.test.yaml up -d",
    "test:db:down": "docker compose -f docker-compose.test.yaml down -v",
    "test:e2e": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test",
    "test:e2e:ui": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test --ui",
    "test:e2e:headed": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test --headed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "prisma:update": "yarn prisma:generate && yarn prisma:migrate",
    "unused": "knip",
    "unused:files": "knip --include files",
    "unused:exports": "knip --include exports",
    "unused:deps": "knip --include dependencies",
    "unused:fix": "knip --fix",
    "clean": "yarn unused:fix && yarn lint:fix",
    "ts-watch": "tsc --noEmit --watch",
    "ts": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "6.8.2",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@react-email/components": "^0.0.42",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.80.6",
    "@trpc/client": "^11.3.1",
    "@trpc/next": "^11.3.1",
    "@trpc/react-query": "^11.3.1",
    "@trpc/server": "^11.3.1",
    "@types/node": "^20",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "resend": "^4.5.2",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.0",
    "happy-dom": "^15.11.7",
    "jsdom": "^27.2.0",
    "knip": "^5.70.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "vite": "^7.2.4",
    "vitest": "^3.0.0",
    "vitest-mock-extended": "^3.1.0"
  }
}
