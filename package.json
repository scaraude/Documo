{
  "name": "documo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && prisma migrate dev && next dev --turbopack | npx pino-pretty --ignore pid,hostname",
    "dev:clean": "rm -rf .next && yarn dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test jest --config=jest.integration.config.js",
    "test:integration:watch": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test jest --config=jest.integration.config.js --watch",
    "test:setup-db": "node scripts/setup-test-db.js",
    "test:teardown-db": "node scripts/teardown-test-db.js",
    "test:seed": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test npx tsx prisma/seed-test.ts",
    "test:workflow": "bash scripts/test-workflow.sh",
    "test:e2e": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test",
    "test:e2e:ui": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test --ui",
    "test:e2e:headed": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/documo_test playwright test --headed",
    "test:e2e:workflow": "bash scripts/e2e-workflow.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "unused": "unimported",
    "unused:files": "unimported --show-preset files",
    "unused:exports": "unimported --show-preset exports",
    "unused:summary": "unimported --show-preset summary",
    "unused:fix": "unimported --update",
    "clean": "yarn unused:fix && yarn lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ts-watch": "tsc --noEmit --watch",
    "ts": "tsc --noEmit",
    "check-env": "npx tsx scripts/check-env.ts",
    "check-env:dev": "npx tsx scripts/check-env.ts .env.development",
    "check-env:test": "npx tsx scripts/check-env.ts .env.test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@react-email/components": "^0.0.42",
    "@tanstack/react-query": "^5.80.6",
    "@trpc/client": "^11.3.1",
    "@trpc/next": "^11.3.1",
    "@trpc/react-query": "^11.3.1",
    "@trpc/server": "^11.3.1",
    "@types/webcrypto": "^0.0.30",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "resend": "^4.5.2",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.52.0",
    "@quramy/jest-prisma": "^1.8.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5",
    "unimported": "^1.31.1"
  }
}
